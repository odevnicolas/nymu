{
  "expo": {
    "name": "nymu",
    "slug": "nymu",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "nymu",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mihiirdev.nymu"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.mihiirdev.nymu"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/logoNymu-01.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF"
        }
      ],
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "O app precisa de acesso às suas fotos para você poder escolher uma foto de perfil."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "87ad6ef0-6ee0-4331-aa36-f83069343cd3"
      },
      "apiUrl": "http://172.20.10.2:3000"
    },
    "owner": "mihiirdev",
    "updates": {
      "url": "https://u.expo.dev/87ad6ef0-6ee0-4331-aa36-f83069343cd3"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
